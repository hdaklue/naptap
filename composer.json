{
    "name": "hdaklue/naptab",
    "description": "NapTab, A Laravel Livewire package for intelligent tabbed navigation with true lazy loading. Tabs stay asleep until activated, waking only when needed.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Ibrahim",
            "email": "hassan@daklue.com"
        }
    ],
    "keywords": [
        "laravel",
        "livewire",
        "tabs",
        "components",
        "ui",
        "rtl",
        "tailwind"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "blade-ui-kit/blade-heroicons": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hdaklue\\NapTab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hdaklue\\NapTab\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hdaklue\\NapTab\\Providers\\NapTabServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
